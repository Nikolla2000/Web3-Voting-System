generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(sequence())
  username String
  email String
  country String
  password String
  IsActive Boolean @default(true)
}

model Car {
  id Int @id @default(sequence())
  brand String
  model String
  horsePower String
}
